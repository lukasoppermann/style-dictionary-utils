{
  fgColor: {
    link: {
      $value: '{base.color.pink.500}',
      $type: 'color',
      $description: 'The link color',
    },
  },
  dimension: {
    px: {
      $value: {
        value: 8,
        unit: 'px',
      },
      $type: 'dimension',
      $description: 'Pixel dimension value',
    },
    rem: {
      $value: {
        value: 0.5,
        unit: 'rem',
      },
      $type: 'dimension',
      description: 'REM dimension value',
    },
  },
  duration: {
    ms: {
      $value: {
        value: 300,
        unit: 'ms',
      },
      $type: 'duration',
      $description: 'The default duration for animations in milliseconds',
    },
    s: {
      $value: {
        value: 0.3,
        unit: 's',
      },
      $type: 'duration',
      $description: 'The default duration for animations in seconds',
    },
  },
  border: {
    default: {
      $value: {
        color: '{base.color.pink.500}',
        style: '{strokeStyle.dashed}',
        width: {
          value: 2,
          unit: 'px',
        },
      },
      $type: 'border',
      $description: 'The default border color',
    },
    muted: {
      $value: {
        color: {
          colorSpace: 'srgb',
          components: [0.5, 0.5, 0.5],
        },
        style: 'solid',
        width: "{dimension.px}",
      },
      $type: 'border',
      $description: 'The default border color',
    },
  },
  number: {
    small: {
      $value: 1.25,
      $type: 'number',
      $description: 'A small float number value',
    },
    large: {
      $value: 1000,
      $type: 'number',
      $description: 'A large integer number value',
    },
  },
  cubicBezier: {
    easeInOut: {
      $value: [0.42, 0, 0.58, 1],
      $type: 'cubicBezier',
      $description: 'An ease-in-out cubic bezier curve',
    },
  },
  transition: {
    fast: {
      $value: {
        duration: '{duration.ms}',
        delay: {
          value: 50,
          unit: 'ms',
        },
        timingFunction: "{cubicBezier.easeInOut}",
      },
      $type: 'transition',
      $description: 'A fast transition',
    },
    slow: {
      $value: {
        duration: {
          value: 1,
          unit: 's',
        },
        delay: {
          value: 300,
          unit: 'ms',
        },
        timingFunction: [0.2, 0, 0.7, 1],
      },
      $type: 'transition',
      $description: 'A fast transition',
    },
  },
  fontWeight: {
    regular: {
      $value: 400,
      $type: 'fontWeight',
      $description: 'Regular font weight',
    },
    bold: {
      $value: "bold",
      $type: 'fontWeight',
      $description: 'Bold font weight',
    },
  },
  fontFamily: {
    primary: {
      $value: "Helvetica, Arial, sans-serif",
      $type: 'fontFamily',
      $description: 'Primary font family',
    },
  },
  fontSize: {
    base: {
      $value: {
        value: 16,
        unit: 'px',
      },
      $type: 'dimension',
      $description: 'Base font size',
    },
    tiny: {
      $value: "{dimension.px}",
      $type: 'dimension',
      $description: 'Tiny font size',
    },
  },
  typography: {
    heading: {
      $value: {
        fontFamily: '{fontFamily.primary}',
        fontSize: '{fontSize.base}',
        fontWeight: '{fontWeight.bold}',
        lineHeight: 1.5,
        letterSpacing: {
          value: 0.1,
          unit: 'px',
        },
      },
      $type: 'typography',
      $description: 'Heading typography style',
    },
  },
  strokeStyle: {
    solid: {
      $value: 'solid',
      $type: 'strokeStyle',
      $description: 'A solid stroke style',
    },
    dashed: {
      $value: 'dashed',
      $type: 'strokeStyle',
      $description: 'A dashed stroke style',
    },
  },
  gradient: {
    primary: {
      $value: [
          { color: '{base.color.pink.500}', position: 0 },
          { color: '{base.color.pink.500}', position: 0.42 },
          { color: '{base.color.pink.600}', position: 1 },
      ],
      $type: 'gradient',
      $description: 'Primary linear gradient',
    },
  },
  shadow: {
    light: {
      $value: {
        offsetX: "{dimension.px}",
        offsetY: {
          value: 2,
          unit: 'px',
        },
        blur: {
          value: 4,
          unit: 'px',
        },
        spread: {
          value: 0,
          unit: 'px',
        },
        color: {
          colorSpace: 'srgb',
          components: [0, 0, 0],
          alpha: 0.1,
        },
      },
      $type: 'shadow',
      $description: 'Light shadow effect',
    },
    pink: {
      $value: {
        offsetX: "{dimension.px}",
        offsetY: {
          value: 2,
          unit: 'px',
        },
        blur: {
          value: 4,
          unit: 'px',
        },
        spread: {
          value: 0,
          unit: 'px',
        },
        color: '{base.color.pink.500}',
      },
      $type: 'shadow',
      $description: 'Pink shadow effect',
    },
  },
}
